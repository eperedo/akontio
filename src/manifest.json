{
	"name": "skipube",
	"version": "1.0.1",
	"description": "automatically close/skip youtube image/video ads!",
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [
		{
			"matches": ["https://www.youtube.com/*"],
			"js": ["content.js"]
		}
	],
	"permissions": ["activeTab"],
	"manifest_version": 2
}
